        public class Methodss {
      public static  int increase(int x){
           x=x+1;
           return x;
           
      } 
   public static  double average(int x, int y){
       double avg = (x+y)/2.0;
       return  avg; 
   }
    public static void showMessage(){
        System.out.println("Hello!");
         
    } 
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        showMessage();
        int v = 5;
         System.out.println("before v:"+v);
        
        v = increase(v);
        System.out.println("after v:"+v);  
     System.out.println("Please enter two values:");
     int x = input.nextInt();
     int y = input.nextInt();
     double avg = average(x,y);
      System.out.println("the average is " +avg); 
       
     System.out.println("Please enter two values:");
     int a = input.nextInt();
     int b = input.nextInt();
     double avg2 = average(a, b);
      System.out.println("the average is " +avg2);
      
      
      
      Loops....................
          Scanner input = new Scanner(System.in);
        
        int number;
        int total = 0;
        int count;
        
        System.out.println("this program calculates the");
        count = 1;
        while(count<=5){
        System.out.println("enter a number");
        number = input.nextInt();
        total = total+number;
         
        ++count;
        }
        System.out.println("total of five numbers " + total);
        
        System.out.println("this program calculates the");
        int i;
        for(i = 1; i<=5; ++i){
        System.out.println("enter a number");
        number = input.nextInt();
        total = total+number;
        }
        System.out.println("total of five numbers " + total);

            
        
